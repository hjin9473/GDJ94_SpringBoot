<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.winter.app.users.UsersDAO"> 
    	<insert id="register" parameterType="UsersDTO">
        INSERT INTO users (
            username, 
            password, 
            name, 
            email, 
            phone, 
            birth
        ) VALUES (
            #{username}, 
            #{password}, 
            #{name}, 
            #{email}, 
            #{phone}, 
            #{birth}
        )
    </insert>
    	
    </mapper>